<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="charlyp_autocomplete.type.autocomplete.class">Charlyp\AutocompleteBundle\Form\AutocompleteType</parameter>
    </parameters>

    <services>
        <!-- Form -->
        <service id="charlyp_autocomplete.type.autocomplete" class="%charlyp_autocomplete.type.autocomplete.class%">
            <argument type="service" id="router" />
            <tag name="form.type" alias="autocomplete" />
        </service>
    </services>
</container>
