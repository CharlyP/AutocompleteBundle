{
    "name": "charlyp/autocomplete-bundle",
    "type": "symfony-bundle",
    "description": "CharlyP Autocomplete Management Symfony Bundle",
    "keywords": ["autocomplete", "form", "symfony", "jquery", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name":  "Charles Pourcel",
            "email": "ch.pourcel@gmail.com"
        },
        {
            "name":  "Jonathan Beurel",
            "email": "bejonster@gmail.com"
        }
    ],
    "require": {
        "php":                 ">=5.3.3",
        "symfony/form":        "~2.3",
        "symfony/routing":     "~2.3",
        "symfony/http-kernel": "~2.3",
        "components/jquery":   ">=1.8.3",
        "components/jqueryui": ">=1.9.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle":      "~2.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle":      "~2.3"
    },
    "autoload": {
        "psr-0": { "Charlyp\\AutocompleteBundle": "" }
    },
    "target-dir": "Charlyp/AutocompleteBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
